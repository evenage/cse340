<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="header">
    <% if (!locals.loggedin) { %>
      <a title="Click to log in" href="/account/login" class="header">My Account</a>
      <% } else { %>
      <div class="header-container">
        <a title="Click to go to Account Management" href="/account/" class="header" Welcome <%= locals.account_firstname || accountData.account_firstname %>>
        </a>
        <a title="Click to log out" href="/account/logout" class="header">Logout</a>
      </div>
     <% } %>
  </div>
</header>
