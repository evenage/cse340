<% if (title) {%>
    <h1>
        <%= title %>
    </h1>
    <%} else {res.redirect('/')} %>

        <%- messages() %>

            <% if (errors) { %>
                <ul class="message warning">
                    <% errors.array().forEach(error=> { %>
                        <li>
                            <%= error.msg %>
                        </li>
                        <% }) %>
                </ul>
                <% } %>

                    <h2>Welcome <%= accountData.account_firstname %>
                    </h2>
                    <!--<p>You're logged in.</p> -->
                    <a href="/account/update/<%= accountData.account_id %>"
                        title="Click to update your account information">Edit Account Information</a>

                    <% if (accountData.account_type !=='Client' ) { %>
                        <h3>Inventory Management</h3>
                        <p><a href="/inv/">Manage Inventory</a></p>
                        <% } %>